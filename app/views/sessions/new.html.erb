<h2>Signup/Login</h2>
<br>
<br>
<%= form_tag sessions_path do %>
    <legend>Please Log In</legend>

    <div class="field">


        <%= label_tag :email, 'Email' %>
        <%= email_field_tag :email, params[:email] %>
      </div>
    <div class="field">
        <%= label_tag :password,'Password'%>
        <%= password_field_tag :password, params[:password] %>
      </div>

      <div class="actions">
        <%=submit_tag "Log In"%>
      </div>


    </div>
<% end %>
