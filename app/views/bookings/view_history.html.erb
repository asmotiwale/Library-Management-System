<p id="notice"><%= notice %></p>

<h1>Booking History for <%= @user.name %></h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Booking start time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.room.number %></td>
        <td><%= booking.booking_start_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Manage Library Members', manage_members_users_url %>